// practice1,2
#include <iostream>

int main()
{
    int a = 10+ 1 * 50 - 40;
    std::cout << a << std::endl;
    int b = (10 + 1) * (50 - 40); // 括弧の中から計算
    std::cout << b << std::endl;
}

